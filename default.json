	"assigneesFromCodeOwners": true,
	"labels": ["dependencies"],
	"packageRules": [
		{
			"matchDepTypes": ["optionalDependencies"],
			"addLabels": ["optional"]
		},
		{
			"extends": "monorepo:dotnet",
			"groupName": "NuGet Packages",
			"matchDatasources": ["nuget"],
			"matchUpdateTypes": [
				"digest",
				"patch",
				"minor",
				"major"
			]
		},
		{
			"groupName": "GitHub Packages",
			"matchDatasources": ["github-tags","github-releases"],
			"automerge": true
		},
		{
			"groupName": "Npm Packages",
			"matchDatasources": ["npm"]
		},
		{
			"groupName": "Docker Packages",
			"matchDatasources": ["docker"]
		},
		{
			"groupName": "Go Packages",
			"matchDatasources": ["go"]
		},
		{
			"groupName": "Hex Packages",
			"matchDatasources": ["hex"]
		},
		{
			"groupName": "Python Packages",
			"matchDatasources": ["pypi"]
		},
		{
			"groupName": "Helm Packages",
			"matchDatasources": ["helm"]
		},
		{
			"groupName": "Rust Packages",
			"matchManagers": ["cargo"]
		},
		{
			"groupName": "Elixir Packages",
			"matchManagers": ["mix"]
		},
		{
			"groupName": "DisCatSharp",
			"matchPackagePatterns": [ "DisCatSharp", "discatsharp", "Discatsharp" ]
		}
	],
	"docker-compose": {
		"major": {
			"enabled": true
		},
		"minor": {
			"enabled": true
		}
	},
	"dockerfile": {
		"major": {
			"enabled": true
		},
		"minor": {
			"enabled": true
		}
	}
}
